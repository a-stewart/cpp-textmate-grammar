{
    "name": "better-cpp-syntax",
    "displayName": "Better C++ Syntax",
    "description": "The bleeding edge of C-like syntaxes",
    "version": "1.15.6",
    "icon": "icon.png",
    "scripts": {},
    "keywords": [
        "C++",
        "cpp",
        "c",
        "objective-c",
        "objective-cpp",
        "objective-c++",
        "syntax",
        "textmate",
        "highlighting",
        "coloring",
        "color"
    ],
    "engines": {
        "vscode": "^1.0.0"
    },
    "author": {
        "name": "Jeff Hykin"
    },
    "publisher": "jeff-hykin",
    "contributes": {
        "grammars": [
            {
                "language": "cpp",
                "scopeName": "source.cpp.embedded.macro",
                "path": "./syntaxes/cpp.embedded.macro.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.assembly.cpp": "asm"
                }
            },
            {
                "language": "cpp",
                "scopeName": "source.cpp",
                "path": "./syntaxes/cpp.tmLanguage.json",
                "embeddedLanguages": {
                    "meta.embedded.assembly.cpp": "asm",
                    "meta.preprocessor.macro.contents": "cpp"
                }
            }
        ]
    },
    "dependencies": {
        "chalk": "^2.4.2",
        "dictionary-en-us": "^2.1.1",
        "lodash": "^4.17.19",
        "nspell": "^2.1.2",
        "oniguruma": "^7.2.0",
        "textmate-bailout": "^1.1.0",
        "textmate-tester": "^1.1.0"
    }
}
